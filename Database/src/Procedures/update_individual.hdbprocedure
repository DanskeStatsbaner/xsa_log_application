PROCEDURE "DataWareHouse.Database.Procedures::update_individual"(
IN
ID INTEGER,
USERNAME NVARCHAR(500),
EMAIL_ADDRESS NVARCHAR(500)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN SEQUENTIAL EXECUTION

UPDATE "DataWareHouse.Database.Tables::log.mail_individual" SET "USERNAME" = :USERNAME, "EMAIL_ADDRESS" =:EMAIL_ADDRESS WHERE "ID" = :ID;

END
